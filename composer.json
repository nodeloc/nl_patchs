{
    "name": "nodeloc/nl-patchs",
    "description": "Nodeloc Patchs",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0",
        "ianm/follow-users": "*",
        "flarum/likes": "^1.8",
        "flarum/tags": "^1.8",
        "xypp/flarum-localize-date-lib": "*",
        "xypp/forum-quests": "*",
        "nodeloc/lottery": "*",
        "clarkwinkelmann/flarum-ext-money-rewards": "^1.0"
    },
    "authors": [
        {
            "name": "Nodeloc",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nodeloc\\NlPatchs\\": "src/"
        },
        "files": [
            "overrides/follow-users/UserPolicy.php",
            "overrides/likes/SaveLikesToDatabase.php"
        ]
    },
    "extra": {
        "flarum-extension": {
            "title": "Nodeloc Patches",
            "category": "feature",
            "icon": {
                "name": "fas fa-toolbox",
                "color": "#fff",
                "backgroundColor": "#f57f17"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": true,
                "editorConfig": true,
                "styleci": true
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
